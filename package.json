{
  "name": "vuelr",
  "version": "0.0.3",
  "description": "Live editing of Vue components.",
  "author": {
    "email": "jonatan.wackstrom@gmail.com",
    "name": "Jonatan Wackstr√∂m (jonataw)",
    "url": "https://jonatan.net"
  },
  "license": "MIT",
  "homepage": "https://github.com/jonataw/vuelr",
  "bugs": {
    "url": "https://github.com/jonataw/vuelr/issues",
    "email": "jonatan.wackstrom@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonataw/vuelr.git"
  },
  "main": "dist/vuelr.js",
  "browser": "dist/vuelr.esm.js",
  "module": "dist/vuelr.esm.js",
  "types": "dist/vuelr.d.ts",
  "files": [
    "dist/*"
  ],
  "keywords": [
    "vue",
    "live",
    "editor",
    "component"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/* && mkdirp dist && cross-env NODE_ENV=production rollup --config build/rollup.config.js && npm run build:css",
    "build:cjs": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:css": "node build/scss.js && sass ./dist:./dist",
    "publish:npm": "npm i && npm run build && npm publish"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/standalone": "^7.15.3",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/babel__standalone": "^7.1.3",
    "@types/codemirror": "^5.60.2",
    "@types/fs-extra": "^9.0.12",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "codemirror": "^5.62.3",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "fs-extra": "^10.0.0",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.36.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.38.0",
    "sass-loader": "^10.2.0",
    "typescript": "^3.9.10",
    "vue": "^2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-template-compiler": "^2.6.14"
  }
}
